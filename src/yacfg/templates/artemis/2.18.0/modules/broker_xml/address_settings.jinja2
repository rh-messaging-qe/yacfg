                {% if address_setting.config_delete_addresses is defined %}
                <config-delete-addresses>{{ address_setting.config_delete_addresses|overridevalue(address_setting.config_delete_addresses) }}</config-delete-addresses>
                {% endif %}
                {% if address_setting.default_non_destructive is defined %}
                <default-non-destructive>{{ mbool(address_setting.default_non_destructive) }}</default-non-destructive>
                {% endif %}
                {% if address_setting.default_consumer_window_size is defined %}
                <default-consumer-window-size>{{ address_setting.default_consumer_window_size }}</default-consumer-window-size>
                {% endif %}
                {% if address_setting.expiry_delay is defined %}
                <expiry-delay>{{ address_setting.expiry_delay }}</expiry-delay>
                {% endif %}
                {% if address_setting.auto_create_dead_letter_resources is defined %}
                <auto-create-dead-letter-resources>{{ mbool(address_setting.auto_create_dead_letter_resources) }}</auto-create-dead-letter-resources>
