                {% endif %}
                {% if address_setting.max_redelivery_delay is defined %}
                <max-redelivery-delay>{{ address_setting.max_redelivery_delay }}</max-redelivery-delay>
                {% endif %}
                {% if address_setting.default_queue_routing_type is defined %}
